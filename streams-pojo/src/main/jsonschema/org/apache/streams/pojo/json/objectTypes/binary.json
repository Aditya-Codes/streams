{
    "$schema": "http://json-schema.org/draft-03/schema",
    "$license": [
        "http://www.openwebfoundation.org/legal/the-owf-1-0-agreements/owfa-1-0",
        "http://www.apache.org/licenses/LICENSE-2.0"
    ],
    "id": "#",
    "type": "object",
    "title": "binary",
    "extends": {
        "$ref": "../object.json"
    },
    "properties": {
        "objectType": {
            "type": "string",
            "default": "binary"
        },
        "attachments": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "compression": {
                "type": "string",
                "enumeration": [
                  "deflate",
                  "gzip"
                ]
              },
              "data": {
                "type": "string"
              },
              "fileUrl": {
                "type": "string"
              },
              "length": {
                "type": "integer"
              },
              "md5": {
                "type": "string"
              },
              "mimeType": {
                "type": "string"
              }
            }
          }
        }

    }
}